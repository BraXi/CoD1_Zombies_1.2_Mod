{
	nopicmip
	nomipmaps
	{
		map ui_mp/assets/cloud1.tga
		tcMod scale 3 2
		tcMod scroll 0.15 0.15
		depthWrite
	}
	{
		map ui_mp/assets/cloud2.tga
		blendFunc GL_ONE GL_ONE
		//tcMod scale 3 3
		tcMod scale 2 1
		//tcMod rotate 8
		tcMod scroll 0.05 0.05
	}

	{
		map $whiteImage
		rgbgen constLighting ( 0.2 0 0 )
		alphaGen wave sin 0.5 0.5 0.5 .5
		blendFunc GL_SRC_ALPHA GL_ONE
	}
}